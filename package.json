{
  "name": "rocket-thing-backend",
  "version": "0.0.1",
  "description": "backend for rocket-thing",
  "main": "main.ts",
  "scripts": {
    "start": "node .dist/main.js",
    "build": "esbuild main.ts --platform=node --bundle --minify --outfile=.dist/main.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q main.ts\"",
    "prepare": "husky install"
  },
  "author": "caspa2289",
  "license": "MIT",
  "dependencies": {
    "connect-pg-simple": "8.0.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "lodash": "4.17.21",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.10.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.30.0",
    "sequelize-typescript": "2.1.5",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "7.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/lodash": "4.14.192",
    "@types/node": "18.15.11",
    "@types/passport": "1.0.12",
    "@types/passport-local": "1.0.35",
    "@types/uuid": "9.0.1",
    "@types/validator": "13.7.14",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "concurrently": "8.0.1",
    "dotenv": "16.0.3",
    "esbuild": "0.17.15",
    "eslint": "8.37.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts, js}": "prettier --write"
  }
}
